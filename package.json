{
  "name": "galvanized-pukeko-ai-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && npm run build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "oxlint . --fix -D correctness --ignore-path .gitignore && eslint . --fix",
    "format": "prettier --write src/",
    "server-build": "tsc --project tsconfig.server.json",
    "server-stdio": "npm run server-build && cd server && gth chat",
    "server-random-components": "npm run server-build && node dist-server/websockets-server.js",
    "server": "npm run server-build && node dist-server/http-server.js",
    "chat": "cd server && gth chat"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "@types/ws": "^8.18.1",
    "express": "^4.21.1",
    "vue": "^3.5.13",
    "ws": "^8.18.2",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.5"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/eslint-plugin": "^1.1.39",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-oxlint": "^0.16.0",
    "eslint-plugin-vue": "~10.0.0",
    "gaunt-sloth-assistant": "^0.6.1",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "oxlint": "^0.16.0",
    "prettier": "3.5.3",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  }
}
